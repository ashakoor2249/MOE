<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Master_of_Emails.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Master_of_Emails"
    xmlns:pages="clr-namespace:practice.Pages"
    Shell.FlyoutBehavior="Disabled" >

    <TabBar>
        <ShellContent Title="Master Of Emails" ContentTemplate="{DataTemplate local:MainPage}" Route="MainPage" />
        <ShellContent Title="Database" ContentTemplate="{DataTemplate pages:DatabasePage}" Route="DatabasePage" />
        <ShellContent Title="Settings" ContentTemplate="{DataTemplate pages:Settings}" Route="Settings" />
        <ShellContent Title="On-Call Schedule" ContentTemplate="{DataTemplate pages:OnCallPage}" Route="OnCallPage" />
    </TabBar>

</Shell>
